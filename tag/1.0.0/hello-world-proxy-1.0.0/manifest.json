{
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": ["./js/background.js"]
  },
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "popup.html",
    "default_title": "遨游加速器"
  },

  "name": "Hello_World",
  "short_name": "Hello_World",
  "description": "Hello World! This is a tool to see the world on internet.",
  "version": "1.0.0",

  "options_page": "popup.html",

  "icons": {
    "128": "images/logo.png"
  },
  "permissions": ["storage", "tabs", "notifications", "management", "proxy", "webRequest", "webRequestBlocking", "http://*/", "https://*/"]
}