{
  // Required
  "manifest_version": 2,
  "name": "Hello World",
  "description": "",
  "version": "0.0.1",

  // Recommended
  "description": "Hello World! This is a tool to see the world on internet.",
  "icons": {
    "128": "images/logo.png"
  },

  // Pick one (or none)
  "browser_action": {
    "default_title": "",
    "default_icon": "images/logo_gray.png",
    "default_popup": "popup.html"
  },

  "options_page": "popup.html",

  "background": {
    "persistent": true,
    "scripts": ["./js/background.js"]
  },
  "permissions": ["storage", "tabs", "notifications", "management", "proxy", "webRequest", "webRequestBlocking", "http://*/", "https://*/"]
}
