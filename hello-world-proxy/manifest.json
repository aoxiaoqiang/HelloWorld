{
    "manifest_version": 2,
    "background":
    {
        "persistent": true,
        "scripts": ["./js/jquery.js", "./js/common.js", "./js/background.js"]
    },
    "browser_action":
    {
        "default_icon": "images/logo.png",
        "default_popup": "popup.html",
        "default_title": "Hello World Proxy"
    },

    "name": "Hello World",
    "short_name": "Hello World",
    "description": "知识无国界，科技改变生活！",
    "version": "1.0.4",

    "options_page": "popup.html",

    "icons":
    {
        "128": "images/logo.png"
    },
    "key": "",
    "permissions": ["storage", "tabs", "notifications", "management", "proxy", "webRequest", "webRequestBlocking", "http://*/", "https://*/"]
}